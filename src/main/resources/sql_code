 create table logins (
     id int generated by default as identity primary key,
     application varchar(255) not null,
     app_account_name varchar(255) not null,
     is_active varchar(255) not null,
     job_title varchar(255) not null,
     department varchar(255) not null
 );

create table postings(
    id int generated by default as identity primary key,
    math_doc varchar(255) not null,
    item int not null,
    doc_date varchar(255) not null,
    pstng_date varchar(255) not null,
    material_description varchar(255) not null,
    quantity int not null,
    bun varchar(255) not null,
    amount_lc varchar(255) not null,
    crcy varchar(255) not null,
    user_name varchar(255) not null,
    is_authorized_delivery boolean
);